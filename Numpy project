import numpy as np
ar=np.array([[1,2,3,4,5],[4,5,6,7,8]])
br=np.array([[[1,2,3],[4,5,6]],[[1,2,3],[4,5,6]]])
print(ar)
print(type(ar))
print(ar.ndim)
print(br.ndim)
